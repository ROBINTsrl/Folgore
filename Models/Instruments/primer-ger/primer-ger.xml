<?xml version="1.0"?>
<PropertyList>

 <path>primer-ger.ac</path>

 <animation>
  <type>translate</type>
  <object-name>button</object-name>
  <property>controls/engines/engine/primer-pressed</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>0.01</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>-1.0</z>
  </axis>
 </animation>

<animation>
  <type>pick</type>
  <object-name>button</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/primer</property>
    <step>0.2</step>
    <min>0</min>
    <max>1.0</max>
    <wrap>false</wrap>
   </binding>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine/primer-pressed</property>
    <value>true</value>
    </binding>
        <mod-up>
          <binding>
            <command>property-assign</command>
      <property>controls/engines/engine/primer-pressed</property>
    <value>false</value>
    </binding>
        </mod-up>
  </action>
</animation>

</PropertyList>


