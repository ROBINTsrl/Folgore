<?xml version="1.0"?>
<PropertyList>

 <path>shotcounter.ac</path>

 <animation>
  <type>material</type>
  <object-name>shot</object-name>
  <object-name>Blende</object-name>
  <object-name>shotrear1</object-name>
  <emission>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
    <red>0.25</red>
    <green>0.25</green>
    <blue>0.8</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>drum</object-name>
  <property>ai/submodels/submodel[1]/count</property>
  <factor>0.65</factor>
   <center>
    <x-m>-0.002</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
