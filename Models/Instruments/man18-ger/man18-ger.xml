<?xml version="1.0"?>
<PropertyList>

  <path>man18-ger.ac</path>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>needle</object-name>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>0.25</red>
      <green>0.25</green>
      <blue>0.8</blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine/mp-osi</property> 
    <interpolation>
      <entry><ind>17.952</ind><dep>15</dep></entry> <!-- 0.6 atm -->
      <entry><ind>53.856</ind><dep>345</dep></entry> <!-- 1.8 atm -->
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>
