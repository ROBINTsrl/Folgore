<?xml version="1.0"?>
<PropertyList>

 <path>gearswitch-ger.ac</path>


 <animation>
  <type>translate</type>
  <object-name>gear_out</object-name>
  <property>controls/gear/gear-down</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>-0.02</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.016</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 <animation>
  <type>translate</type>
  <object-name>gear_in</object-name>
  <property>controls/gear/gear-down</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-0.02</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.016</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 <animation>
    <type>pick</type>
    <object-name>gear_in</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/gear/gear-down</property>
    <step>-1</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
    </action>
 </animation>
 <animation>
    <type>pick</type>
    <object-name>gear_out</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/gear/gear-down</property>
    <step>1</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
    </action>
 </animation>


</PropertyList>


