<?xml version="1.0"?>
<PropertyList>

 <path>zsk244.ac</path>

 <animation>
  <type>material</type>
  <object-name>socket</object-name>
  <object-name>face</object-name>
  <object-name>selector</object-name>
  <emission>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
    <red>0.1</red>
    <green>0.1</green>
    <blue>0.5</blue>
  </emission>
 </animation>

 <animation>
  <type>select</type>
  <object-name>lamp</object-name>
  <condition>
        <not-equals>
          <property>controls/armament/station[0]/fuse-type</property>
          <value>2</value>
        </not-equals>
  </condition>
 </animation>
	<animation>
		<type>material</type>
		<object-name>lamp</object-name>
	  <emission>
	   <red>1.0</red>
	   <green>0.0</green>
	   <blue>0.0</blue>
	  </emission>
	  <shininess>1</shininess>
	</animation>

<animation>
  <type>rotate</type>
  <object-name>selector</object-name>
  <property>controls/armament/station[0]/fuse-type</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-140</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>-60</dep>
   </entry>
   <entry>
    <ind>2</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>3</ind>
    <dep>60</dep>
   </entry>
   <entry>
    <ind>4</ind>
    <dep>140</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-0.0</x-m>
   <y-m>-0.0</y-m>
   <z-m>0.034</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


  <animation>
    <type>pick</type>
    <object-name>selector</object-name>
    <visible>true</visible>
    <action>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/armament/station[0]/fuse-type</property>
        <step>1</step>
        <min>0</min>
        <max>4</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>selector</object-name>
    <visible>true</visible>
    <action>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/armament/station[0]/fuse-type</property>
        <step>-1</step>
        <min>0</min>
        <max>4</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>

</PropertyList>


