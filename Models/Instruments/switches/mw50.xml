<?xml version="1.0"?>
<PropertyList>

 <path>switch-ger.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>switch</object-name>
  <property>controls/engines/engine[0]/mw-50</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>60</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-0.002</x-m>
   <y-m>0.0</y-m>
   <z-m>0.00</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
<animation>
  <type>pick</type>
  <object-name>switch</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine[0]/mw-50</property>
   </binding>
  </action>
</animation>
</PropertyList>


