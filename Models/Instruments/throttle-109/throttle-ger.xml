<?xml version="1.0"?>
<PropertyList>

 <path>throttle-ger.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>throttle</object-name>
  <property>controls/engines/engine[0]/throttle</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>60</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.04</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.01</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>rotate</type>
  <object-name>propauto</object-name>
  <property>controls/engines/engine[0]/prop-auto</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>-20</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>20</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.023</x-m>
   <y-m>0.005</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1.0</z>
  </axis>
 </animation>
 <animation>
  <type>rotate</type>
  <object-name>cutoff</object-name>
  <property>controls/fuel/switch-position</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>2.0</ind>
    <dep>60</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.04</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.01</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
  <type>pick</type>
  <object-name>propauto</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine[0]/prop-auto</property>
   </binding>
  </action>
</animation>
<animation>
  <type>pick</type>
  <object-name>throttlefwd</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/throttle</property>
    <step>0.1</step>
    <min>0</min>
    <max>1.0</max>
    <wrap>false</wrap>
   </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>throttleback</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/throttle</property>
    <step>-0.1</step>
    <min>0</min>
    <max>1.0</max>
    <wrap>false</wrap>
   </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>cutofffwd</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/fuel/switch-position</property>
    <step>1.0</step>
    <min>-1.0</min>
    <max>1.0</max>
    <wrap>false</wrap>
   </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>cutoffback</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/fuel/switch-position</property>
    <step>-1.0</step>
    <min>-1.0</min>
    <max>1.0</max>
    <wrap>false</wrap>
   </binding>
  </action>
</animation>

</PropertyList>


